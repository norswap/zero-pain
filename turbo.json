{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["build/**"]
    },
    "transit": {
      "dependsOn": ["^transit"]
    },
    "clean": {
      "cache": false
    },
    "make": {
      "dependsOn": ["^make"]
    },
    "typecheck": {
      "dependsOn": ["^transit"]
    }
  }
}